[
    Anvil:
    - A library for writing anvil instructions and general data.
]

[
    Workspace
    - The part that does the binary writing.
]

[ create a workspace ]
!scourge.anvil.workspace
(
    do_write !scourge.cell,
    write_to !scourge.cell,
    length !scourge.cell,
    program !scourge.buffer
)

[ open a workspace ]
scourge.anvil.workspace.open()(workspace !scourge.anvil.workspace) = {
    scourge.pack.null()(workspace !scourge.anvil.workspace)
}

[ advance workspace ]
scourge.anvil.workspace.setup_pass(workspace !scourge.anvil.workspace, do_write !scourge.cell)(workspace !scourge.anvil.workspace) = {
    [ setup do_write ]
    scourge.copy(do_write)(workspace:do_write)

    [ determine pass type ]
    scourge.integer.within_range(scourge.false, workspace:do_write, scourge.false)(measure_pass, not_measure_pass)
    scourge.integer.within_range(scourge.true, workspace:do_write, scourge.true)(write_pass, not_write_pass)

    [ if is starting a measure pass ]
    @measure_pass measure_pass = {
        [ setup length ]
        scourge.copy(scourge.constant.0)(workspace:length)
    }

    [ if is starting a write pass ]
    @write_pass write_pass = {
        [ allocate buffer based on length ]
        scourge.buffer.request(workspace:length)(workspace:program)

        [ setup write to ]
        scourge.copy(workspace:program:start)(workspace:write_to)

        [ reset length ]
        scourge.copy(scourge.constant.0)(workspace:length)
    }
}

[ write cell data to workspace ]
scourge.anvil.workspace.write.cell(workspace !scourge.anvil.workspace, data !scourge.cell, count !scourge.cell)(workspace !scourge.anvil.workspace) = {
    [ if is write pass, do write and advancement ]
    @do_write workspace:do_write = {
        [ write data & advance ]
        scourge.cell_to_address(data, count, workspace:write_to)(workspace:write_to)
    }

    [ advance length ]
    scourge.integer.add(workspace:length, count)(workspace:length)
}

[ write buffer to workspace ]
scourge.anvil.workspace.write.buffer(workspace !scourge.anvil.workspace, data !scourge.buffer)(workspace !scourge.anvil.workspace) = {
    [ calculate buffer length ]
    scourge.buffer.calculate.length(data)(data.length)

    [ if is write pass, do write and advancement ]
    @do_write workspace:do_write = {
        [ calculate destination buffer ]
        scourge.buffer.calculate.end_address(workspace:write_to, data.length)(destination.end)
        scourge.pack(workspace:write_to, destination.end)(destination !scourge.buffer)

        [ write data ]
        scourge.buffer.copy.low_to_high(data, destination)()

        [ advance current ]
        scourge.integer.add(workspace:write_to, data.length)(workspace:write_to)
    }

    [ advance length ]
    scourge.integer.add(workspace:length, data.length)(workspace:length)
}

